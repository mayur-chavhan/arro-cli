# System Settings
TZ=UTC

# User/Group IDs (run 'id -u' and 'id -g' to get your values)
PUID=1000
PGID=1000

# Theme Configuration (theme.park)
# Options: dark, nord, dracula, aquamarine, plex, maroon, space-gray
TP_THEME=nord

# Paths Configuration
# Configuration files location
CONFIG_ROOT=./config
 # Media storage location (absolute path)
STORAGE_ROOT=/home/dietpi/media-storage
 # Base URL path (e.g., /media or empty for root)
BASE_PATH=/

# Domain Configuration
# For local IP access, use: 192.168.1.11.nip.io (replace with your IP)
# For localhost: localhost
# For custom domain: media.yourdomain.com
DOMAIN=localhost
ACME_EMAIL=test@domain.com

# Ports Configuration
TRAEFIK_PORT=80

# Media Management
DELETE_AFTER_SEED=false

# Download Categories
QB_CATEGORY_TV=tv-sonarr
QB_CATEGORY_MOVIES=movies-radarr

# Container Paths (don't change these)
MOVIES_PATH=/movies
SERIES_PATH=/tv
BOOKS_PATH=/books
MUSIC_PATH=/music
DOWNLOADS_PATH=/downloads

# Jellystat Configuration
JELLYSTAT_DB_PASSWORD=jellystat
JELLYSTAT_JWT_SECRET=change-me-in-production

# Cloudflare Tunnel (optional - uncomment to enable)
# TUNNEL_TOKEN=your-tunnel-token-here

# API Keys (will be auto-generated)
SONARR_API_KEY=
RADARR_API_KEY=
PROWLARR_API_KEY=
JACKETT_API_KEY=
BAZARR_API_KEY=
SEERR_API_KEY=
JELLYFIN_API_KEY=

# Homepage Environment Variables
HOMEPAGE_VAR_JELLYFIN_API_KEY=
HOMEPAGE_VAR_SEERR_API_KEY=
HOMEPAGE_VAR_RADARR_API_KEY=
HOMEPAGE_VAR_SONARR_API_KEY=
HOMEPAGE_VAR_BAZARR_API_KEY=
HOMEPAGE_VAR_PROWLARR_API_KEY=
HOMEPAGE_VAR_JACKETT_API_KEY=
HOMEPAGE_VAR_JELLYSTAT_API_KEY=